<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/Grey_500">

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_margin="20dp"
        android:onClick="onTempTestFlowButtonClick" />

    <ImageView
        android:id="@+id/image_dot_back"
        android:layout_width="14px"
        android:layout_height="14px"
        android:layout_centerInParent="true"
        android:src="@drawable/goods_black_dot" />

    <ImageView
        android:id="@+id/image_dot"
        android:layout_width="20px"
        android:layout_height="20px"
        android:layout_centerInParent="true"
        android:src="@drawable/goods_white_dot" />

    <com.song.sunset.widget.MultiCircleView
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

    <com.song.sunset.widget.TouchFollowView
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_centerInParent="true" />

    <com.song.sunset.widget.TextSwitchView
        android:id="@+id/textswitch"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true" />

    <com.song.sunset.widget.CircleProgress
        android:id="@+id/circle_progress"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        app:bg_circle_color="@color/Grey_400"
        app:bg_circle_stroke_width="1.5"
        app:circle_color="@color/colorPrimary"
        app:circle_speed="9"
        app:circle_stroke_width="4"
        app:current_angle_rate="0" />

    <!--<ImageView-->
    <!--android:id="@+id/ic_svg"-->
    <!--android:layout_width="200dp"-->
    <!--android:layout_height="200dp"-->
    <!--android:layout_centerHorizontal="true"-->
    <!--android:src="@drawable/hahah"/>-->

